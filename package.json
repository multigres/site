{
  "name": "multigres.com",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build && npx tsx scripts/generate-author-rss.ts",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "deploy-preview": "npm run build && cd build && rm -f CNAME && git init && git checkout -b gh-pages && git add -A && git commit -m 'Deploy preview' && git push -f git@github.com:sougou/site.git gh-pages:gh-pages",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md,mdx}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md,mdx}\""
  },
  "dependencies": {
    "@docusaurus/core": "3.2.1",
    "@docusaurus/plugin-content-docs": "3.2.1",
    "@docusaurus/plugin-google-analytics": "^3.6.3",
    "@docusaurus/plugin-google-gtag": "^3.6.3",
    "@docusaurus/preset-classic": "3.2.1",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "gsap": "^3.13.0",
    "lucide-react": "^0.365.0",
    "posthog-js": "^1.261.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.2.1",
    "@docusaurus/theme-classic": "^3.8.1",
    "@docusaurus/tsconfig": "3.2.1",
    "@docusaurus/types": "3.2.1",
    "@eslint/js": "^9.31.0",
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "@types/js-yaml": "^4.0.5",
    "globals": "^16.4.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.6.2",
    "tsx": "^4.7.0",
    "typescript": "~5.2.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  },
  "packageManager": "pnpm@9.15.5+sha1.cb1f6372ef64e2ba352f2f46325adead1c99ff8f",
  "pnpm": {
    "overrides": {
      "webpack-dev-server@<=5.2.0": ">=5.2.1",
      "@eslint/plugin-kit@<0.3.4": ">=0.3.4",
      "on-headers@<1.1.0": ">=1.1.0"
    }
  }
